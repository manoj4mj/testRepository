{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mdatafactories"
		},
		"LSSql_order_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LSSql_order'"
		},
		"demoBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'demoBlobStorage'"
		},
		"msmSqlDatabase_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'msmSqlDatabase'"
		},
		"LSKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://msmtestkeyvault.vault.azure.net/"
		},
		"msmDataLakeStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://msmdatalakegen2.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mdatafactories_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LSSql_order_connectionString": {
						"value": "[parameters('LSSql_order_connectionString')]"
					},
					"demoBlobStorage_connectionString": {
						"value": "[parameters('demoBlobStorage_connectionString')]"
					},
					"msmSqlDatabase_connectionString": {
						"value": "[parameters('msmSqlDatabase_connectionString')]"
					},
					"LSKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('LSKeyVault_properties_typeProperties_baseUrl')]"
					},
					"msmDataLakeStorage_properties_typeProperties_url": {
						"value": "[parameters('msmDataLakeStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}